#include <iostream>

#include "std_lib_facilities.h"

using namespace std;

//Implement square() without using multiplication operator; do repeated addition

int archaic_square(int v) {
    int total = 0;

    for (int i = 0; i < v; ++i) {
        total += v;
    }

    return total;
}

int main()
{
    for (int i = 0; i < 100; ++i) {
        cout << i << '\t' << archaic_square(i) << '\n';
    }
}
